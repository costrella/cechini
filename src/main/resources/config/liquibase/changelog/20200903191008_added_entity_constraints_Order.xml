<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Order.
    -->
    <changeSet id="20200903191008-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="worker_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_worker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="worker"/>

        <addForeignKeyConstraint baseColumnNames="store_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_store_id"
                                 referencedColumnNames="id"
                                 referencedTableName="store"/>

        <addForeignKeyConstraint baseColumnNames="status_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="status"/>

        <addForeignKeyConstraint baseColumnNames="warehouse_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_warehouse_id"
                                 referencedColumnNames="id"
                                 referencedTableName="warehouse"/>

    </changeSet>
</databaseChangeLog>
